<div class="login">
  <div class="container">
    <div class="form">

    <h1>Sign up</h1>
		<%= form_for @user do |f| %>
			First name: <%= f.text_field :first_name %><br />
      Last name : <%= f.text_field :last_name %><br />
			Email     : <%= f.text_field :email %><br />
      Password  : <%= f.text_field :password %> <br/>
      <%= f.submit "Singup", class: "btn btn-large btn-primary" %>
    <% end %>

  
    </div>
  </div>
</div>